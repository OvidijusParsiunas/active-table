{
  "name": "edi-table",
  "version": "0.0.1",
  "description": "A fully editable table",
  "main": "edi-table.js",
  "module": "edi-table.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf edi-table.{d.ts,d.ts.map,js,js.map} test/edi-table.{d.ts,d.ts.map,js,js.map} test/edi-table_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch"
  },
  "keywords": [
    "table",
    "grid",
    "editable",
    "edit",
    "react",
    "vue",
    "angular",
    "javascript",
    "typescript",
    "add",
    "remove",
    "component",
    "web-components",
    "lit",
    "lit-element"
  ],
  "author": "Ovidijus Parsiunas",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OvidijusParsiunas/edi-table.git"
  },
  "bugs": {
    "url": "https://github.com/OvidijusParsiunas/edi-table/issues"
  },
  "homepage": "https://cssfilterconverter.com",
  "dependencies": {
    "lit": "^2.2.4"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@open-wc/testing": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@web/dev-server": "^0.1.31",
    "@web/dev-server-legacy": "^1.0.0",
    "@web/test-runner": "^0.13.27",
    "@web/test-runner-playwright": "^0.8.8",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "eslint": "^8.15.0",
    "lit-analyzer": "^1.2.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "~4.7.4"
  },
  "customElements": "custom-elements.json"
}
